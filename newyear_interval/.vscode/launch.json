            "program": "${workspaceFolder}/bin/newyear_interval",
            "args": ["-a", "1.csv", "-b", "2.csv", "-t1e-6", "-m/home/shaowb/Dropbox/stdcurvesTestNewtonOnestep/bin/", "-n/home/shaowb/Dropbox/stdcurvesTestNewtonOnestep/bin/"],
